{
    "name": "custom-snippets",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": ["csharp", "cs"],
                "path": "./snippets/csharp/csharp.json"
            },
            {
                "language": "csharpdoc",
                "path": "./snippets/csharp/csharpdoc.json"
            }
        ]
    }
}

